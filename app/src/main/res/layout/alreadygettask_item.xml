<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/layoutset_20_10_20_10_mw">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:orientation="vertical">

        <TextView
            android:id="@+id/alreadygettask_item_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:maxLength="15"
            android:text="诚聘高薪餐饮兼职日结"
            android:textSize="@dimen/font_size_16" />

        <TextView
            android:id="@+id/alreadygettask_item_adressanddata"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/space_5"
            android:gravity="center"
            android:text="南京市 鼓楼区 2018/04/05"
            android:textSize="@dimen/font_size_12" />
    </LinearLayout>

    <TextView
        android:id="@+id/alreadygettask_item_complete"
        android:layout_width="@dimen/space_40"
        android:layout_height="@dimen/space_25"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:background="@drawable/textview_round"
        android:gravity="center"
        android:text="完成"
        android:textColor="@color/white"
        android:textSize="@dimen/font_size_12" />

</RelativeLayout>
